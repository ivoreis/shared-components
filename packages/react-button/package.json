{
  "private": false,
  "name": "@ivoreis/react-button",
  "version": "0.1.2-alpha.244",
  "description": "Example button component",
  "author": "Ivo Reis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivoreis/shared-components.git"
  },
  "scripts": {
    "build": "pika build",
    "dev": "parcel example/index.html --open",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "test:all": "npm run test && npm run type-check && npm run build"
  },
  "dependencies": {
    "theme-ui": "^0.3.1"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-build-types"
      ],
      [
        "@pika/plugin-bundle-types"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/pkg"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  }
}
